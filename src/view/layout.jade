doctype html
html
	head
		title Janux Authorization
		meta(name='viewport', content='width=device-width, initial-scale=1')

		
		link(rel='stylesheet', href='css/normalize.css')
		link(rel='stylesheet', href='css/typography.css')
		link(rel='stylesheet', href='css/util.css')
		link(rel='stylesheet', href='css/layout.css')
		link(rel='stylesheet', href='css/responsive.css')

	body
		.container
			header
				.logo
					img(src='img/janux-sage-256.png') 
					.branding
						span.janux janux 
						span.people people

				.auth

					.auth-status Logout 
						span.entypo.entypo-logout

					.auth-user Philippe Paravicini &nbsp;
						span.entypo.entypo-down-open

			nav
				ul
					li.first: a(href='#accounts') Accounts
					li: a(href='#roles') Roles
					li: a(href='#perms') Permissions




			section.sidebar

				block sidebar
					include sidenav

			section.maincolumn

				block main-content
				block footer
					include google-analytics

